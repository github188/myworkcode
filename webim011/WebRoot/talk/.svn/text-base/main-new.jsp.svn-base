  	 	function sleep(){
  	 		if(window.top.document.readyState!="undefined"){
  	 			setTimeout(function(){
  	 				ui.init("${basePath}");
  	 			},1000);
  	 			return;
  	 		}
  	 		if(window.top.document.readyState!="complete"){
  				setTimeout("sleep()",1000);  			  				
  			}else{
  				ui.init("${basePath}");
  			}
  	 	}


